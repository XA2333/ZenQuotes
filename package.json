{
  "name": "quote-of-the-day",
  "private": true,
  "version": "1.0.0",
  "description": "A beautiful, modern web application that displays daily inspirational quotes using the ZenQuotes API. Features include favorites, theme switching, and social sharing.",
  "author": "Your Name",
  "type": "module",
  "keywords": ["react", "typescript", "vite", "quotes", "inspiration", "daily-quotes"],
